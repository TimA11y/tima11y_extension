{
  "manifest_version": 3,
  "name": "TimA11y Extension",
  "description": "TimA11y Productivity Extension",
  "version": "0.1.0",
  "action": {
    "default_popup": "./popups/popup.html"
  },
  "content_scripts": [
    {
      "matches": [
        "http://*/*",
        "https://*/*"
      ],
      "css": [
        "/contents/tima11y.css"
      ],
      "js": [
        "contents/content.js"
      ]
    }
  ]
}